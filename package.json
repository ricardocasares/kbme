{
  "name": "kbme",
  "version": "2.1.0",
  "description": "Gather kanban metrics from your JIRA instance",
  "main": "lib/index.js",
  "bin": {
    "kbme": "cli.js"
  },
  "engines": {
    "node": ">=7.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardocasares/kbme.git"
  },
  "bugs": {
    "url": "https://github.com/ricardocasares/kbme/issues"
  },
  "author": "Ricardo Casares",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.0",
    "dotenv": "^7.0.0",
    "got": "^9.2.0",
    "mri": "^1.1.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^10.2.1"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "npx semantic-release@beta",
    "commit": "npx git-cz",
    "format": "npx prettier --write **/*.js"
  },
  "release": {
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  }
}
